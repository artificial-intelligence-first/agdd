  "title": "SalaryBandLookupOutput",
  "description": "Output from salary band lookup skill",
  "type": "object",
  "required": ["currency", "min", "max", "source"],
  "properties": {
    "currency": {
      "type": "string",
      "description": "Currency code (e.g., 'USD', 'EUR')"
    },
    "min": {
      "type": "number",
      "description": "Minimum salary in the band",
      "minimum": 0
    },
    "max": {
      "type": "number",
      "description": "Maximum salary in the band",
      "minimum": 0
    },
    "source": {
      "type": "string",
      "description": "Data source (e.g., 'database', 'fallback', 'api')"
    },
    "metadata": {
      "type": "object",
      "description": "Additional metadata about the lookup",
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
