{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://magsag.ai/schemas/skills/result-aggregation/input.schema.json",
  "title": "ResultAggregationInput",
  "description": "Input for result aggregation skill",
  "type": "object",
  "required": ["results"],
  "properties": {
    "results": {
      "type": "array",
      "description": "Array of results from sub-agents to aggregate",
      "items": {
        "type": "object",
        "properties": {
          "task_id": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "output": {
            "type": "object"
          }
        }
      }
    }
  },
  "additionalProperties": false
}
