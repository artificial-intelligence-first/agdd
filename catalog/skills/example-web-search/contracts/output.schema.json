{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://agdd.ai/schemas/skills/example-web-search/output.schema.json",
  "title": "WebSearchResultOutput",
  "description": "Output from web search skill",
  "type": "object",
  "required": ["url", "success"],
  "properties": {
    "url": {
      "type": "string",
      "format": "uri",
      "description": "URL that was fetched"
    },
    "success": {
      "type": "boolean",
      "description": "Whether the fetch was successful"
    },
    "title": {
      "type": "string",
      "description": "Page title"
    },
    "content": {
      "type": "string",
      "description": "Page content (text or HTML)"
    },
    "error": {
      "type": "string",
      "description": "Error message if fetch failed"
    },
    "metadata": {
      "type": "object",
      "description": "Additional metadata about the fetch operation",
      "required": ["status_code", "content_type"],
      "properties": {
        "status_code": {
          "type": "integer",
          "description": "HTTP status code"
        },
        "content_type": {
          "type": "string",
          "description": "Content-Type header value"
        },
        "extracted_text": {
          "type": "boolean",
          "description": "Whether text was extracted from HTML"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
